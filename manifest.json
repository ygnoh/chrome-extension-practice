{
    "manifest_version": 2,

    "name": "Hello world",
    "description": "This is my first extension",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "author": "yonggoo.noh@gmail.com",

    "permissions": ["tabs", "<all_urls>"],

    "content_scripts": [
        {
            "matches": [
                "*://*.github.com/*/issues"
            ],
            "js": ["changeButton.js"],
            "css": ["changeButton.css"]
        }
    ]
}